{
  "openapi": "3.0.1",
  "info": {
    "title": "Cat API",
    "description": "API for managing cats",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Generated server url"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
